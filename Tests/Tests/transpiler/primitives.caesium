template Token<_token>:
    type Token:
        String value
        Int n_indent

template Until<T>:
    type Until:
        Int n_indent

type IndentToken:
	Int n_indent

template Indent<T>:
    type Indent : public `T`:
        Indent(Int n_indent) : `T`(n_indent + 1) {}

template Alloc<T>:
    type Alloc:
        Box<`T`> value
        Int n_indent

template And<Ands...>:
    type And:
        Tuple<`Ands...`> value

template Or<Ors...>:
    type Or:
        Variant<`Ors...`> _value

type TemplateBody:
	Int n_indent
	String value
